  "author": "Gordon Larrigan <gordonlarrigan@gmail.com> (https://gordonlarrigan.com)",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basementuniverse/world-locations-map.git"
  },
  "license": "MIT",
  "keywords": [
    "world",
    "map",
    "geo",
    "location"
  ],
  "devDependencies": {
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "copyfiles": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "react-resize-detector": "^6.7.1"
  }
